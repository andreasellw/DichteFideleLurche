<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" spacing="30.0" xmlns="http://javafx.com/javafx/8.0.112"
      fx:controller="view.client.robFromOthersPopUP.robFromOthersController">
    <children>
        <HBox alignment="CENTER" maxWidth="500.0" minHeight="150.0" minWidth="500.0" prefHeight="150.0"
              prefWidth="500.0" spacing="5.0">
            <children>
                <ImageView fx:id="arrowIn" fitHeight="75.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true"/>
                <ComboBox fx:id="playerChoices" prefHeight="60.0" prefWidth="324.0" promptText="Select one player!"/>
                <VBox maxHeight="75.0" maxWidth="75.0" minHeight="75.0" minWidth="75.0" prefHeight="75.0"
                      prefWidth="75.0"/>
            </children>

        </HBox>
        <Button fx:id="okButton" maxWidth="0.0" minHeight="40.0" minWidth="100.0" mnemonicParsing="false"
                prefHeight="40.0" prefWidth="100.0" text="OK"/>
    </children>
    <stylesheets>
        <URL value="@/css/catanBoxes.css"/>
    </stylesheets>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</VBox>
